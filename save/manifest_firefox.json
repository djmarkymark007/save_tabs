{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Save and load your current set of tabs",
  "permissions": ["tabs", "storage", "<all_urls>"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": "assets/ok.png",
    "default_popup": "html/popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tabmanager@example.com"
    }
  },
  "icons": {
    "96": "assets/ok.png"
  },
  "content_security_policy": {
    "extension_pages": "default-src 'self'; script-src 'self'; style-src 'self'; connect-src http://192.168.254.146:8080"
  }
}