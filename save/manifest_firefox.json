{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Save and load your current set of tabs",
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": "assets/ok.png",
    "default_popup": "html/popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tabmanager@example.com"
    }
  },
  "icons": {
    "96": "assets/ok.png"
  }
}